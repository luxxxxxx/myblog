<script src="./js/jqq.js"></script>
<script>
    let $email = $("#email");
    $(".getCode").click(function () {
        console.log(1)
        Ajax({
            'method' : 'post',
            'url' : '/reg/sendMail',
            'data' : {
                'email' : $email.val()
            },
            success : function (data) {
                console.log(data);
            }
        })
    })
</script>